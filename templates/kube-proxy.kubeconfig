#jinja2: lstrip_blocks: True, trim_blocks: True
---
apiVersion: v1
kind: Config
clusters:
  - name: "{{ control_plane_kubernetes_cluster_name }}"
    cluster:
      server: "{{ control_plane_api_external_endpoint }}"
      certificate-authority-data: {{ control_plane_kubernetes_ca_cert | string | b64encode }}
contexts:
  - name: default
    context:
      cluster: "{{ control_plane_kubernetes_cluster_name }}"
      user: "system:kube-proxy"
current-context: default
preferences: {}
users:
  - name: "system:kube-proxy"
    user:
      client-certificate-data: {{ control_plane_kube_proxy_cert | string | b64encode }}
      client-key-data: {{ control_plane_kube_proxy_key | string | b64encode }}
